class RestaurantRankingComponent {
    constructor(dialog) {
        this.dialog = dialog;
    }
    ngOnChanges(changes) {
        if (!changes.restaurants$.currentValue) {
            return;
        }
        changes.restaurants$.currentValue
            .pipe(Object(rxjs_operators__WEBPACK_IMPORTED_MODULE_1__["map"])((restaurants) => {
            const sortResult = restaurants.sort(this.sortByScore);
            this.sortedBestRestaurants = sortResult.slice(0, 3);
            this.sortedRestOfRestaurants = sortResult.slice(3) ? sortResult.slice(3) : [];
        })).subscribe();
    }
    sortByScore(a, b) {
        if (a.votes > b.votes) {
            return -1;
        }
        else if (a.votes < b.votes) {
            return 1;
        }
        else {
            return 0;
        }
    }
    setRankLabel(restaurant) {
        return restaurant.votes <= 1 ? `<b>${restaurant.votes} vote</b> pour ${restaurant.name}` : `<b>${restaurant.votes} votes</b> pour ${restaurant.name}`;
    }
    openConfirmDialog(restaurant) {
        this.openDialog(restaurant);
    }
    openDialog(restaurant) {
        const dialogRef = this.dialog.open(_modal_component__WEBPACK_IMPORTED_MODULE_2__["ModalComponent"], {
            width: '400px',
            data: Object.assign({}, restaurant)
        });
        dialogRef.afterClosed().subscribe((result) => {
            this.modalResult = result;
        });
    }
}
RestaurantRankingComponent.ɵfac = function RestaurantRankingComponent_Factory(t) { return new (t || RestaurantRankingComponent)(_angular_core__WEBPACK_IMPORTED_MODULE_0__["ɵɵdirectiveInject"](_angular_material_dialog__WEBPACK_IMPORTED_MODULE_3__["MatDialog"])); };
