class RestaurantService {
    constructor(firestore) {
        this.firestore = firestore;
    }
    createRestaurant(name) {
        return this.firestore.collection('restaurants').add({
            name,
            createdAt: Date.now(),
            votes: 0
        });
    }
    readRestaurants() {
        return this.firestore.collection('restaurants', (ref) => ref.orderBy('name', 'asc'));
    }
    voteForRestaurant(restaurant) {
        return this.firestore.doc(`restaurants/${restaurant.id}`).update(Object.assign(Object.assign({}, restaurant), { votes: restaurant.votes + 1 }));
    }
    deleteRestaurant(restaurant) {
        return this.firestore.doc(`restaurants/${restaurant.id}`).delete();
    }
    resetAllRestaurants() {
        return Object(tslib__WEBPACK_IMPORTED_MODULE_0__["__awaiter"])(this, void 0, void 0, function* () {
            yield this.firestore.collection("restaurants").get().toPromise().then(function (querySnapshot) {
                querySnapshot.forEach(function (doc) {
                    doc.ref.update({
                        votes: 0
                    });
                });
            });
        });
    }
}
